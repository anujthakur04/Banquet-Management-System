            <thead>
                <tr>
                    
                        <th>Name</th>
                        <th>Email</th>
                        <th>Date</th>
                        <th>Banquet</th>
                        <th>Amount</th>
                        
                </tr>
            </thead>
            <% records.forEach(function(row){%>
            
            <tr>
                <td><%=row.name%></td>
                <td><%=row.email%></td>
                <td><%=row.date%></td>
                <td><%=row.banquet%></td>
                <td><%=row.amount%></td>
                </tr>

<%})%>

            
        </table>
